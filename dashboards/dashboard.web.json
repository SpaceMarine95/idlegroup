{
  "uid": "web-logs",
  "title": "Web â€“ Nginx Overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "tags": ["web", "nginx", "loki"],
  "templating": {
    "list": [
      {
        "name": "status_class",
        "type": "query",
        "datasource": { "type": "loki", "uid": "grafana" },
        "query": "label_values({job=\"nginx\"}, status_class)",
        "includeAll": true,
        "multi": true
      },
      {
        "name": "method",
        "type": "query",
        "datasource": { "type": "loki", "uid": "grafana" },
        "query": "label_values({job=\"nginx\"}, method)",
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "RPS (last 1m)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(({job=\"nginx\"}[1m])))",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" } }
    },
    {
      "type": "timeseries",
      "title": "Requests by status class",
      "gridPos": { "x": 6, "y": 0, "w": 12, "h": 8 },
      "options": { "legend": { "displayMode": "table", "placement": "right" } },
      "fieldConfig": { "defaults": { "custom": { "stacking": { "mode": "normal" } } } },
      "targets": [
        {
          "expr": "sum by (status_class)(rate(({job=\"nginx\", status_class=~\"$status_class\"} | json)[1m]))",
          "legendFormat": "{{status_class}}xx",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Avg request time (5m)",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "avg_over_time(({job=\"nginx\"} | json | unwrap req_time)[5m])",
          "legendFormat": "avg req_time",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Method share (10m)",
      "gridPos": { "x": 0, "y": 8, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum by (method)(count_over_time(({job=\"nginx\", method=~\"$method\"} | json)[10m]))",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    },
    {
      "type": "table",
      "title": "Top 5xx URIs (1h)",
      "gridPos": { "x": 6, "y": 8, "w": 9, "h": 8 },
      "options": { "showHeader": true },
      "targets": [
        {
          "expr": "topk(10, count_over_time(({job=\"nginx\"} | json | status =~ \"5..\" | line_format '{{.uri}}')[1h]))",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    },
    {
      "type": "table",
      "title": "Top User-Agents (10m)",
      "gridPos": { "x": 15, "y": 8, "w": 9, "h": 8 },
      "options": { "showHeader": true },
      "targets": [
        {
          "expr": "topk(10, count_over_time(({job=\"nginx\"} | json | line_format '{{.ua}}')[10m]))",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    },
    {
      "type": "logs",
      "title": "Recent 5xx errors",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 10 },
      "options": { "showLabels": true, "showTime": true },
      "targets": [
        {
          "expr": "{job=\"nginx\"} | json | status =~ \"5..\"",
          "refId": "A",
          "datasource": { "type": "loki", "uid": "grafana" }
        }
      ]
    }
  ]
}